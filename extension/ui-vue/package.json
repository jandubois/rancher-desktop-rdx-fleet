{
  "name": "fleet-extension-ui-vue",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "lint": "eslint src --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed"
  },
  "dependencies": {
    "@docker/extension-api-client": "^0.3.4",
    "@mdi/font": "^7.4.47",
    "color-namer": "^1.4.0",
    "colorthief": "^2.6.0",
    "js-yaml": "^4.1.1",
    "jszip": "^3.10.1",
    "pinia": "^3.0.3",
    "pro-color-harmonies": "^0.8.0",
    "vue": "^3.5.16",
    "vue-markdown-render": "^2.2.1",
    "vuedraggable": "^4.1.0",
    "vuetify": "^3.8.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@playwright/test": "1.56.1",
    "@testing-library/vue": "^8.1.0",
    "@types/js-yaml": "^4.0.9",
    "@types/jszip": "^3.4.0",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.39.2",
    "eslint-plugin-vue": "^10.1.0",
    "globals": "^16.0.0",
    "jsdom": "^27.3.0",
    "msw": "^2.12.3",
    "sass": "^1.89.4",
    "typescript": "^5.3.0",
    "vite": "^6.3.0",
    "vite-plugin-vuetify": "^2.1.2",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.10"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
